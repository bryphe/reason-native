{
  "name": "@reason-native/chalk-console",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "pesy",
    "release": {
      "releasedBinaries": [
        "ChalkConsoleApp.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": ["chalk-console.lib"],
      "main": "TestChalkConsole",
      "name": "TestChalkConsole.exe"
    },
    "library": {
      "name": "chalk-console.lib",
      "namespace": "ChalkConsole",
      "require": ["console.lib", "chalk.lib"]
    }
  },
  "scripts": {
    "test": "esy x TestChalkConsole.exe"
  },
  "dependencies": {
    "@reason-native/console": "*",
    "@reason-native/chalk": "*",
    "@opam/dune": "*",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": "~4.2.3",
    "pesy": "*"
  },
  "devDependencies": {
    "@esy-ocaml/merlin": "*",
    "ocaml": "~4.6.0",
    "@reason-native/console": "link:../console",
    "@reason-native/chalk": "link:../chalk"
  }
}
